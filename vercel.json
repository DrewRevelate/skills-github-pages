{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/next" }
  ],
  "github": {
    "enabled": true,
    "silent": false,
    "autoJobCancelation": true,
    "deploymentEnabled": {
      "main": true,
      "gh-pages": false
    }
  },
  "cleanUrls": true,
  "trailingSlash": true,
  "env": {
    "NEXT_PUBLIC_MOCK_SUPABASE": "true"
  }
}
